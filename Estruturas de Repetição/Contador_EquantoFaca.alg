algoritmo "contar ate quanto"
// Função :
// Autor :
// Data : 04/04/2022
// Seção de Declarações 
var
   cont, val: inteiro
inicio
      cont <- 0
      escreva("Quer contar até quanto? ")
      leia(val)
      
      enquanto (cont <= val) faca
         escreval(cont)
         cont <- cont + 1
      fimenquanto
      escreval("Terminei a contagem")



fimalgoritmo
