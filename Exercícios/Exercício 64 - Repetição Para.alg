algoritmo "CONTADOR - REPETIÇÃO PARA"
// Função : exercicio 64
// Autor :
// Data : 18/04/2022
// Seção de Declarações 
var
   c: inteiro
inicio
   para c <- 0 ate 100 passo 5 faca
      escreva(c)
   fimpara
   escreval(" Acabou!")
   escreval("")
   para c <- 100 ate 0 passo -5 faca
      escreva(c)
   fimpara
   escreval(" Acabou!")

fimalgoritmo
