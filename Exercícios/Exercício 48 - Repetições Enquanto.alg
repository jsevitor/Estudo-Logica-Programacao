algoritmo "somador acumulativo"
// Função : exercicio 48
// Autor :
// Data : 13/05/2022
// Seção de Declarações 
var
   i, x, S: inteiro
inicio
   i <- 1
   enquanto (i <= 7) faca
      escreva("Digite um valor: ")
      leia(x)
      S <- S + x
      i <- i + 1
   fimenquanto
   escreval()
   escreval("Resultado da soma =",S)
fimalgoritmo
