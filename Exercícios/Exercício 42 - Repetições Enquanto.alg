algoritmo "contador"
// Função : exercicio 42
// Autor :
// Data : 13/05/2022
// Seção de Declarações 
var
   i: inteiro
   x: inteiro
inicio
   escreva("Digite um valor: ")
   leia(x)
   i <- 1
   
   enquanto (i <= x) faca
      escreva(i)
      i <- i + 1
   fimenquanto
   escreval(" Acabou!")
fimalgoritmo
