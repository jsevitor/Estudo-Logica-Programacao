algoritmo "TESTE VETOR"
// Função :
// Autor :
// Data : 19/04/2022
// Seção de Declarações 
var
   v: vetor[1..6] de inteiro
   c: inteiro
inicio
   para c <- 1 ate 6 faca
      escreva("Digite o",c,"º valor: ")
      leia(v[c])
   fimpara
   
   para c <- 1 ate 6 faca
      escreva("{",v[c],"}")
   fimpara
   
   
fimalgoritmo
