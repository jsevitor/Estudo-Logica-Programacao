algoritmo "dobro e terça parte"
// Função :
// Autor :
// Data : 24/03/2022
// Seção de Declarações 
var
   num :  real
   dob, ter: real
inicio
      escreva("Digite um número: ")
      leia(num)
      dob <- num * 2
      ter <- num / 3
      escreval("O dobro de", num," é", dob)
      escreval("A terça parte de", num, " é", ter)
fimalgoritmo
